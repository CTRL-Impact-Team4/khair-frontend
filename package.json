{
	"name": "sopo-ui",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"packageManager": "yarn@4.3.1",
	"engines": {
		"yarn": "4.3.1"
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "tsc --noEmit -p tsconfig.json && biome lint --diagnostic-level=error --no-errors-on-unmatched ./**/*.{js,jsx,ts,tsx}",
		"lint:fix": "biome check --write --unsafe ./**/*.{js,jsx,ts,tsx}",
		"lint:staged": "biome check --error-on-warnings --no-errors-on-unmatched --staged ./**/*.{js,jsx,ts,tsx}",
		"preview": "vite preview"
	},
	"dependencies": {
		"@radix-ui/react-context-menu": "^2.2.1",
		"@uidotdev/usehooks": "^2.4.1",
		"copy-to-clipboard": "^3.3.3",
		"file-type": "^19.3.0",
		"lucide-react": "^0.412.0",
		"rc-tree": "^5.8.8",
		"rc-upload": "^4.6.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.25.1",
		"usehooks-ts": "^3.1.0",
		"uuid": "^10.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@tailwindcss/typography": "^0.5.13",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@types/uuid": "^10",
		"@typescript-eslint/eslint-plugin": "^7.15.0",
		"@typescript-eslint/parser": "^7.15.0",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"autoprefixer": "^10.4.19",
		"postcss": "^8.4.39",
		"tailwindcss": "^3.4.6",
		"ts-node": "^10.9.2",
		"typescript": "^5.2.2",
		"vite": "^5.3.4"
	}
}
